location / {
	proxy_pass	http://localhost:8888;
	proxy_http_version	1.1;
	proxy_set_header	Upgrade $http_upgrade;
	proxy_set_header	Connection $connection_upgrade;
}

location /wstats {
	proxy_pass	http://localhost:8889;
	proxy_http_version	1.1;
	proxy_set_header	Upgrade $http_upgrade;
	proxy_set_header	Connection $connection_upgrade;
}

location /stats {
	root	/var/www/echo/static;
	index	index.html;
}

